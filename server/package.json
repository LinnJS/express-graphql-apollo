{
  "name": "express-graphql-apollo-server",
  "version": "0.1.0",
  "description": "NodeJS Express server with GraphQL endpoint using Apollo",
  "main": "server.js",
  "author": "LinnJS <linnjsconsulting@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"nodemon src/server.ts\" \"npm run generate\"",
    "generate": "graphql-codegen",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "npm run build && node dist/server.js",
    "start:prod": "node dist/server.js",
    "prepare": "npm run generate"
  },
  "dependencies": {
    "apollo-server": "^3.10.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.0",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-resolvers": "^2.7.3",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/graphql": "14.5.0",
    "@types/node": "18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "concurrently": "7.4.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.1",
    "jsdoc": "3.6.11",
    "nodemon": "2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  }
}
