{
  "name": "express-graphql-apollo-server",
  "version": "0.1.0",
  "description": "NodeJS Express server with GraphQL endpoint using Apollo",
  "main": "server.js",
  "author": "LinnJS <linnjsconsulting@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"nodemon src/server.ts\" \"npm run generate\"",
    "generate": "graphql-codegen",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "npm run build && node dist/server.js",
    "start:prod": "node dist/server.js",
    "prepare": "npm run generate"
  },
  "dependencies": {
    "apollo-server-express": "^2.21.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-subscriptions": "^1.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.8",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "^1.23.0",
    "@graphql-codegen/typescript-resolvers": "^1.20.0",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/graphql": "14.5.0",
    "@types/node": "14.18.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "concurrently": "5.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "jsdoc": "3.6.7",
    "nodemon": "2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "9.1.1",
    "typescript": "4.5.4"
  }
}
